<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/stylesheets/style.css">
    <title><%= title %> </title>
</head>
<body>
    <span class="planet-order">
        <span class="order">->O<-</span>

    </span>
    <span style="margin-left: 20px;"class="planet-disorder">
        <span  class="order">O--></span>

    </span>

     <span class="info hide">
       <h2 style="margin-bottom: 5px;" class="planet-name"></h2>
       <p style="max-width: 10px; font-family: Georgia, 'Times New Roman', Times, serif; margin: 0;" class="planet-type"></p>
       <p class="planet-velrot"></p>
       <p class="planet-diam"></p>
       <p class="planet-temp"></p>
       <p class="planet-mass"></p>
       <p class="planet-density"></p>
       <p class="planet-desc"></p>
    </span> 
   

    <div class="solar-system">

        <div class="star-1"></div>
        <div class="star-2"></div>
        <div class="star-3"></div>
        <div class="star-4"></div>
        <div class="star-5"></div>
        <div class="star-6"></div>
        <div class="star-7"></div>
        <div class="star-8"></div>

        <div class="sun"></div>
        <div class="mercury"></div>
        <div class="mars"></div>
        <div class="earth"></div>
        <div class="venus"></div>
        <div class="jupiter"></div>
        <div class="saturn"></div>
        <div class="uranus"></div>
        <div class="neptune"></div>
        <div class="pluto"></div>
    </div>
    <div class="kuiper-belt">
        <div class="asteroid-1"></div>
        <div class="asteroid-2"></div>
        <div class="asteroid-3"></div>
        <div class="asteroid-4"></div>
        <div class="asteroid-5"></div>
        <div class="asteroid-6"></div>
        <div class="asteroid-7"></div>
        <div class="asteroid-8"></div>
        <div class="asteroid-9"></div>
        <div class="asteroid-10"></div>
    </div>


    <script src="/javascripts/main.js"></script>
</body>
</html>